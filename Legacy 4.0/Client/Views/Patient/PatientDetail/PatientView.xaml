                                                TabIndex="3" Command="{Binding AddOperationNoteCommand}" CommandParameter="Patient"/>
                                        </WrapPanel>
                                    </StackPanel>
                                </TabItem>
                                <TabItem Header="Medical" IsSelected="{Binding MPViewModel.IsSelected, Mode=TwoWay}">
                                    <StackPanel>
                                        <Grid ShowGridLines="True">
                                            <Grid.RowDefinitions>
