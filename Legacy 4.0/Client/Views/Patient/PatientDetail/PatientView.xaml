                                        </StackPanel>
                                    </StackPanel>
                                    <StackPanel Orientation="Vertical"  Margin="5" HorizontalAlignment="Left" Grid.ColumnSpan="1" Grid.Column="3">
                                        <StackPanel Orientation="Vertical">
                                            <TextBlock Text="Home Phone No"/>
